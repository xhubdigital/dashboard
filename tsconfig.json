{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "experimentalDecorators": true,
    "useDefineForClassFields": true,
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@api/*": ["./src/api/*"],
      "@boot/*": ["./src/boot/*"],
      "@components/*": ["./src/components/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@models/*": ["./src/models/*"],
      "@pages/*": ["./src/pages/*"],
      "@store/*": ["./src/store/*"]
    }
  }
}